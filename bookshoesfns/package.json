{
  "name": "functions",
  "scripts": {
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src -c .eslintrc.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
